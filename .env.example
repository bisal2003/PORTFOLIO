# Environment Variables for Contact Form
# Copy this file to .env.local and fill in your actual values

# Contact Information
REACT_APP_CONTACT_EMAIL=bisal.career@gmail.com
REACT_APP_CONTACT_PHONE=+919395360772

# Web3Forms Configuration (Recommended - Free and Reliable)
REACT_APP_WEB3FORMS_ACCESS_KEY=your_web3forms_access_key_here

# Email Service Configuration (Alternative Options)

# Option 1: SendGrid (Recommended for production)
SENDGRID_API_KEY=your_sendgrid_api_key_here
SENDGRID_FROM_EMAIL=contact@yourportfolio.com

# Option 2: EmailJS (Client-side email service)
REACT_APP_EMAILJS_SERVICE_ID=your_emailjs_service_id
REACT_APP_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
REACT_APP_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# Option 3: Formspree (Simple form backend)
REACT_APP_FORMSPREE_ENDPOINT=https://formspree.io/f/xeoqkqpr

# Deployment Settings
REACT_APP_SITE_URL=https://yourportfolio.vercel.app
GENERATE_SOURCEMAP=false
